{
  "name": "demo",
  "description": "conditionnal output bot",
  "intents": [
    {
      "input": ["hello", "hi", "yeah"], 
      "output": [
        {
          "type": "condition",
          "children": [
            {
              "name": "var",
              "value": null,
              "text": "hello<<var=again>>"
            },
            {
              "name": "var",
              "value": "again",
              "text": "hello {{var}}"
            }
          ]
        }
      ] }]
}
